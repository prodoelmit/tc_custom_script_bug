<?xml version="1.0" encoding="UTF-8"?>
<template xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" uuid="f21df099-4506-4f08-aedb-715205ca5edf" xsi:noNamespaceSchemaLocation="https://www.jetbrains.com/teamcity/schemas/2020.1/project-config.xsd">
  <name>Test build</name>
  <settings>
    <options>
      <option name="buildNumberPattern" value="%dep.Test_GetBuildnumber.build.number%" />
      <option name="checkoutMode" value="MANUAL" />
    </options>
    <parameters>
      <param name="branch" value="master" />
    </parameters>
    <build-runners>
      <runner id="RUNNER_169" name="" type="simpleRunner">
        <parameters>
          <param name="plugin.docker.imageId" value="%system.docker.ubuntu.name%" />
          <param name="script.content" value="echo &quot;Hello %build.number%&quot;" />
          <param name="teamcity.step.mode" value="default" />
          <param name="use.custom.script" value="true" />
        </parameters>
      </runner>
    </build-runners>
    <vcs-settings>
      <vcs-entry-ref root-id="Test_GithubHubot" />
    </vcs-settings>
    <requirements>
      <equals id="RQ_52" name="teamcity.agent.jvm.os.name" value="Linux" />
    </requirements>
    <build-triggers />
    <dependencies>
      <depend-on sourceBuildTypeId="Test_GetBuildnumber">
        <options>
          <option name="take-started-build-with-same-revisions" value="true" />
          <option name="take-successful-builds-only" value="true" />
        </options>
      </depend-on>
    </dependencies>
    <cleanup />
  </settings>
</template>

